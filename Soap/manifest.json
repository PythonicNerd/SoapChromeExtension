  "name": "Soap",
  "description": "This extension 'scrubs' bad words and insults from pages.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "soap.png",
    "default_popup": "interface.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
